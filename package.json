{
  "name": "@crystallize/react-layout",
  "description": "Helpers for setting up main layout with side menus",
  "version": "3.2.0",
  "main": "dist/index.js",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "styled-components": ">=3.2.5"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "next": "^9.3.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "release-it": "^12.4.3",
    "styled-components": "^3.2.5",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12"
  },
  "scripts": {
    "dev": "next",
    "build": "webpack --mode=production",
    "prepare": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CrystallizeAPI/react-layout.git"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "module": "./module"
          }
        }
      ],
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  }
}
